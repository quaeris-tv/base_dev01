{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "amenadiel/jpgraph": "*",
        "calebporzio/sushi": "*",
        "cknow/laravel-money": "*",
        "doctrine/dbal": "^3.6",
        "fidum/laravel-eloquent-morph-to-one": "^2.2",
        "genealabs/laravel-model-caching": "^0.13.2",
        "guzzlehttp/guzzle": "^7.5",
        "intervention/image": "^2.7",
        "intervention/imagecache": "^2.6",
        "kalnoy/nestedset": "^6.0",
        "laravel/framework": "^10.0",
        "laravel/passport": "^11.8",
        "laravel/sanctum": "^3.2",
        "laravel/socialite": "^5.6",
        "laravel/tinker": "^2.8",
        "laravel/ui": "*",
        "laravelcollective/html": "^6.4",
        "livewire/livewire": "^2.12",
        "mcamara/laravel-localization": "^1.8",
        "mhmiton/laravel-modules-livewire": "^1.6",
        "nwidart/laravel-modules": "*",
        "spatie/eloquent-sortable": "^4.0",
        "spatie/laravel-cookie-consent": "^3.2",
        "spatie/laravel-data": "^3.3",
        "spatie/laravel-database-mail-templates": "^3.5",
        "spatie/laravel-livewire-wizard": "*",
        "spatie/laravel-medialibrary": "*",
        "spatie/laravel-model-info": "^1.4",
        "spatie/laravel-model-states": "^2.4",
        "spatie/laravel-model-status": "*",
        "spatie/laravel-permission": "^5.10",
        "spatie/laravel-query-builder": "^5.2",
        "spatie/laravel-queueable-action": "^2.14",
        "spatie/laravel-sitemap": "^6.2",
        "spatie/laravel-sluggable": "^3.4",
        "spatie/laravel-tags": "*",
        "staudenmeir/eloquent-has-many-deep": "*",
        "symfony/dom-crawler": "^6.2",
        "unisharp/laravel-filemanager": "^2.6",
        "wire-elements/pro": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.5",
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/",
            "Themes\\": "Themes/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "Modules/Xot/Helpers/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://wire-elements-pro.composer.sh"
        }
    ]
}
